{
  "name": "accountabstraction",
  "version": "0.1.0",
  "description": "",
  "scripts": {
    "clear": "rm -rf cache artifacts typechain typechain-types dist contracts/dist",
    "hardhat-compile": "./scripts/hh-wrapper compile",
    "guardian-deploy": "pnpm hardhat deploy",
    "lint": "pnpm compile && pnpm run lint:sol && pnpm run lint:js ",
    "lint:js": "eslint -f unix .",
    "lint-fix": "eslint -f unix . --fix",
    "lint:sol": "solhint -f unix \"contracts/**/*.sol\" --max-warnings 0",
    "gas-calc": "./scripts/gascalc",
    "mocha-gascalc": "TS_NODE_TRANSPILE_ONLY=1 npx ts-mocha --bail gascalc/*",
    "test": "./scripts/hh-wrapper test",
    "coverage": "COVERAGE=1 hardhat coverage",
    "deploy": "./scripts/hh-wrapper deploy",
    "test-dev": "hardhat test --network dev",
    "ci": "pnpm compile && hardhat test && pnpm run runop",
    "ci-gas-calc": "pnpm gas-calc && pnpm check-gas-reports",
    "check-gas-reports": "./scripts/check-gas-reports",
    "runop": "hardhat run src/runop.ts ",
    "runop-goerli": "AA_URL=https://account-abstraction-goerli.nethermind.io pnpm runop --network goerli",
    "runop3": "hardhat run src/runop3.ts "
  },
  "devDependencies": {
    "@ethersproject/abi": "^5.7.0",
    "@ethersproject/providers": "^5.7.2",
    "@nomiclabs/hardhat-ethers": "^2.2.2",
    "@nomiclabs/hardhat-waffle": "^2.0.3",
    "@typechain/ethers-v5": "^10.2.0",
    "@types/chai": "^4.3.4",
    "@types/node": "^16.18.12",
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "@typescript-eslint/parser": "^5.50.0",
    "chai": "^4.3.7",
    "eslint": "^8.33.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-config-standard-with-typescript": "^21.0.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-standard": "^5.0.0",
    "ethers": "^5.7.2",
    "hardhat": "^2.12.6",
    "solhint": "^3.3.8",
    "ts-generator": "^0.1.1",
    "ts-mocha": "^10.0.0",
    "ts-node": "^10.9.1",
    "typechain": "^8.1.1"
  },
  "dependencies": {
    "@gnosis.pm/safe-contracts": "^1.3.0",
    "@nomiclabs/hardhat-etherscan": "^2.1.8",
    "@openzeppelin/contracts": "^4.8.1",
    "@thehubbleproject/bls": "^0.5.1",
    "@typechain/hardhat": "^2.3.1",
    "@types/mocha": "^9.1.1",
    "big-integer": "^1.6.51",
    "circomlibjs": "^0.0.8",
    "dotenv": "^16.0.3",
    "ethereumjs-util": "^7.1.5",
    "ethereumjs-wallet": "^1.0.2",
    "hardhat-deploy": "^0.9.29",
    "hardhat-deploy-ethers": "^0.3.0-beta.13",
    "solidity-coverage": "^0.7.22",
    "source-map-support": "^0.5.21",
    "table": "^6.8.1",
    "typescript": "^4.9.5"
  }
}
